    "@graphql-codegen/schema-ast": "^4.0.0",
    "@mrleebo/prisma-ast": "^0.8.0",
    "@pothos/core": "^3.38.0",
    "@pothos/plugin-prisma": "^3.61.0",
    "@pothos/plugin-relay": "^3.44.0",
    "@prisma/client": "^5.6.0",
    "aws-sdk": "^2.1555.0",
    "dataloader": "^2.2.2",
